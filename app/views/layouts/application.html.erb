<!DOCTYPE html>
<html>
  <head>
    <title>Gym</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <%= form_tag "/clients/find" do %>
          <%= text_field_tag :search_string %>
          <%= submit_tag :search %>
    <% end %> 
    <%= yield %>
  </body>
</html>
